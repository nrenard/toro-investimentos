{
  "name": "test-toro",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "2.4.3",
    "dotenv": "9.0.2",
    "dynamoose": "2.7.3",
    "express": "4.17.1",
    "helmet": "4.4.1",
    "joi": "17.4.0",
    "jsonwebtoken": "8.5.1",
    "module-alias": "2.2.2",
    "morgan": "1.10.0",
    "swagger-ui-express": "4.1.6",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@shelf/jest-dynamodb": "1.8.1",
    "@types/bcryptjs": "2.4.2",
    "@types/express": "4.17.11",
    "@types/jest": "26.0.22",
    "@types/jsonwebtoken": "8.5.1",
    "@types/node": "14.14.37",
    "@types/supertest": "2.0.11",
    "@types/swagger-ui-express": "4.1.2",
    "@types/uuid": "8.3.0",
    "@typescript-eslint/eslint-plugin": "4",
    "eslint": "7",
    "eslint-config-standard-with-typescript": "20.0.0",
    "eslint-plugin-import": "2",
    "eslint-plugin-node": "11",
    "eslint-plugin-promise": "4",
    "git-commit-msg-linter": "3.1.0",
    "husky": "4.2.5",
    "jest": "26.6.3",
    "lint-staged": "10.5.4",
    "supertest": "6.1.3",
    "ts-jest": "26.5.4",
    "ts-node": "9.1.1",
    "ts-node-dev": "1.1.6",
    "tsconfig-paths": "3.9.0",
    "typescript": "4.2.4"
  },
  "scripts": {
    "build": "npx tsc -p tsconfig-build.json",
    "start": "NODE_BUILD=true node dist/index.js",
    "dev": "NODE_ENV=dev ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules src/index.ts",
    "test": "NODE_ENV=test jest --passWithNoTests --noStackTrace",
    "test:verbose": "NODE_ENV=test jest --passWithNoTests",
    "test:unit": "npm test -- -c jest-unit-config.ts",
    "test:integration": "npm test -- -c jest-integration-config.ts",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
